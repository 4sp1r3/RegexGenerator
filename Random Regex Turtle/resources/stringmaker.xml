<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>Properties for StringMaker Class</comment>
	<entry key="root_directory">resources/rules/SnortModified</entry>
	<entry key="file_filter">.*</entry>
	<entry key="output_directory">resources/out</entry>
	<entry key="string_number_regex">100</entry>
        <!-- Store pattern is not mandatory, but it's useful if you need to store all regex pattern in a file. -->
        <entry key="store_pattern">resources/patterns.dat</entry>
</properties>